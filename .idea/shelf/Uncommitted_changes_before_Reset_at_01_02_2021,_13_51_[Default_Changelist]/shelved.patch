<+>.user_information {\n    display: inline;\n    max-width: 250px;\n    max-height: 130px;\n    border-radius: 30px;\n    box-shadow: 0 9px 50px hsla(20, 67%, 75%, 0.31);\n    padding: 2%;\n    background-image: linear-gradient(-225deg, #e3fdf5 35%, #f89c1c 100%);\n    margin: 1% 1%;\n    vertical-align: top;\n    float: right;\n}\n\n/* Table */\n\ntable.costs_table {\n    font-family: Arial, Courier;\n    font-size: 14pt;\n    background-image: linear-gradient(-225deg, #e3fdf5 35%, #f89c1c 100%);\n    width: 80%;\n    margin: 3% auto;\n    text-align: center;\n    border-collapse: collapse;\n    box-shadow: 0 0 10px 2px #f89c1c;\n    border-radius: 5px;\n    color: #0c7578;\n    border: 1px solid #0c7578;\n    padding: 5px 5px;\n}\n\ntable.costs_table tbody td {\n    font-size: 13px;\n    color: #0c7578;\n}\n\ntable.costs_table tr:nth-child(even) {\n    background: #e3fdf5 100%;\n}\n\ntable.costs_table tfoot {\n    font-size: 12px;\n    font-weight: bold;\n    color: #E6E6E6;\n    background: #000000;\n    background: -moz-linear-gradient(top, #404040 0%, #191919 66%, #000000 100%);\n    background: -webkit-linear-gradient(top, #404040 0%, #191919 66%, #000000 100%);\n    background: linear-gradient(to bottom, #404040 0%, #191919 66%, #000000 100%);\n    border-top: 1px solid #4A4A4A;\n}\n\ntable.costs_table tfoot td {\n    font-size: 12px;\n}\n\n#delete, #edit {\n    padding-left: 10px;\n}\n\n.delete_icon {\n    width: 18px;\n    height: 20px;\n}\n\n.edit_icon {\n    width: 20px;\n    height: 20px;\n}\n\n.add_icon {\n    width: 20px;\n    height: 20px;\n}\n\n.userinfo_icon {\n    width: 25px;\n    height: 25px;\n}\n\n.button_none_btn {\n    background: none;\n    border: none;\n}\n\n.icon_size {\n    width: 20px;\n    height: 20px;\n    margin: -3px 3px;\n    cursor: pointer;\n}
